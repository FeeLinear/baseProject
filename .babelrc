{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "iOS >= 7",
            "Android >= 4.1",
            "last 10 Chrome versions",
            "last 10 Firefox versions",
            "Safari >= 6",
            "ie >= 9"
          ]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx",
    "transform-runtime"
  ],
  "env": {
    "development": {
      "plugins": [
        "dynamic-import-node"
      ]
    }
  }
}